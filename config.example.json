{
  "agents": {
    "defaults": {
      "workspace": "~/.oxicrab/workspace",
      "model": "claude-sonnet-4-5-20250929",
      "maxTokens": 8192,
      "temperature": 0.7,
      "maxToolIterations": 20,
      "sessionTtlDays": 30,
      "memoryIndexerInterval": 300,
      "mediaTtlDays": 7,
      "maxConcurrentSubagents": 5,
      "localModel": null,
      "provider": null,
      "contextProviders": [],
      "compaction": {
        "enabled": true,
        "thresholdTokens": 40000,
        "keepRecent": 10,
        "extractionEnabled": true,
        "model": null,
        "preFlushEnabled": false,
        "checkpoint": {
          "enabled": false,
          "intervalIterations": 10
        }
      },
      "daemon": {
        "enabled": true,
        "interval": 300,
        "executionModel": null,
        "executionProvider": null,
        "strategyFile": "HEARTBEAT.md",
        "maxIterations": 25
      },
      "memory": {
        "archiveAfterDays": 30,
        "purgeAfterDays": 90,
        "embeddingsEnabled": false,
        "embeddingsModel": "BAAI/bge-small-en-v1.5",
        "hybridWeight": 0.5,
        "searchFusionStrategy": "weighted_score",
        "rrfK": 60,
        "embeddingCacheSize": 10000,
        "recencyHalfLifeDays": 90
      },
      "costGuard": {
        "dailyBudgetCents": null,
        "maxActionsPerHour": null,
        "modelCosts": {}
      },
      "promptGuard": {
        "enabled": false,
        "action": "warn"
      },
      "cognitive": {
        "enabled": false,
        "gentleThreshold": 12,
        "firmThreshold": 20,
        "urgentThreshold": 30,
        "recentToolsWindow": 10
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "token": "your-telegram-bot-token",
      "allowFrom": [],
      "dmPolicy": "allowlist"
    },
    "discord": {
      "enabled": false,
      "token": "your-discord-bot-token",
      "allowFrom": [],
      "commands": [
        {
          "name": "ask",
          "description": "Ask the AI assistant",
          "options": [
            {
              "name": "question",
              "description": "Your question or message",
              "required": true
            }
          ]
        }
      ],
      "dmPolicy": "allowlist"
    },
    "slack": {
      "enabled": false,
      "botToken": "xoxb-your-slack-bot-token",
      "appToken": "xapp-your-slack-app-token",
      "allowFrom": [],
      "dmPolicy": "allowlist"
    },
    "whatsapp": {
      "enabled": false,
      "allowFrom": [],
      "dmPolicy": "allowlist"
    },
    "twilio": {
      "enabled": false,
      "accountSid": "your-twilio-account-sid",
      "authToken": "your-twilio-auth-token",
      "phoneNumber": "+1234567890",
      "webhookPort": 8080,
      "webhookPath": "/twilio/webhook",
      "webhookUrl": "https://your-domain.com/twilio/webhook",
      "allowFrom": [],
      "dmPolicy": "allowlist"
    }
  },
  "providers": {
    "anthropic": {
      "apiKey": "sk-ant-your-anthropic-key",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "anthropicOAuth": {
      "enabled": false,
      "accessToken": "",
      "refreshToken": "",
      "expiresAt": 0,
      "credentialsPath": null,
      "autoDetect": true
    },
    "openai": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "gemini": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "openrouter": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "deepseek": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "groq": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "moonshot": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "zhipu": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "dashscope": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "vllm": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "ollama": {
      "apiKey": "",
      "apiBase": null,
      "promptGuidedTools": false
    },
    "circuitBreaker": {
      "enabled": false,
      "failureThreshold": 5,
      "recoveryTimeoutSecs": 60,
      "halfOpenProbes": 2
    }
  },
  "gateway": {
    "enabled": true,
    "host": "0.0.0.0",
    "port": 18790,
    "webhooks": {},
    "a2a": {
      "enabled": false,
      "agentName": "",
      "agentDescription": ""
    }
  },
  "tools": {
    "exfiltrationGuard": {
      "enabled": false,
      "allowTools": []
    },
    "web": {
      "search": {
        "provider": "brave",
        "apiKey": "your-brave-search-api-key",
        "maxResults": 5
      }
    },
    "exec": {
      "timeout": 60,
      "allowedCommands": [
        "ls", "find", "tree", "pwd", "basename", "dirname", "realpath", "stat", "file",
        "cat", "head", "tail", "less", "wc", "md5sum", "sha256sum",
        "grep", "awk", "sed", "sort", "uniq", "cut", "tr", "diff", "comm", "paste",
        "rg", "ag", "fd",
        "jq", "yq",
        "git",
        "cargo", "rustc", "npm", "npx", "node", "python3", "pip3", "make", "go",
        "date", "cal", "whoami", "hostname", "uname", "uptime", "df", "du", "free", "ps",
        "env", "printenv", "which", "type",
        "curl", "wget", "dig", "nslookup", "ping", "host",
        "echo", "printf", "test", "true", "false", "yes", "seq", "xargs",
        "tar", "zip", "unzip", "gzip", "gunzip", "zcat",
        "tee", "touch", "mkdir", "cp", "mv", "ln"
      ],
      "sandbox": {
        "enabled": true,
        "additionalReadPaths": [],
        "additionalWritePaths": [],
        "blockNetwork": true
      }
    },
    "restrictToWorkspace": false,
    "google": {
      "enabled": false,
      "clientId": "your-google-client-id",
      "clientSecret": "your-google-client-secret",
      "scopes": [
        "https://www.googleapis.com/auth/gmail.modify",
        "https://www.googleapis.com/auth/gmail.send",
        "https://www.googleapis.com/auth/calendar.events",
        "https://www.googleapis.com/auth/calendar.readonly"
      ]
    },
    "github": {
      "enabled": false,
      "token": "ghp_your-github-token"
    },
    "weather": {
      "enabled": false,
      "apiKey": "your-openweathermap-api-key"
    },
    "todoist": {
      "enabled": false,
      "token": "your-todoist-api-token"
    },
    "media": {
      "enabled": false,
      "radarr": {
        "url": "http://localhost:7878",
        "apiKey": "your-radarr-api-key"
      },
      "sonarr": {
        "url": "http://localhost:8989",
        "apiKey": "your-sonarr-api-key"
      }
    },
    "obsidian": {
      "enabled": false,
      "apiUrl": "https://127.0.0.1:27124",
      "apiKey": "your-obsidian-local-rest-api-key",
      "vaultName": "MyVault",
      "syncInterval": 300,
      "timeout": 15
    },
    "browser": {
      "enabled": false,
      "headless": true,
      "chromePath": null,
      "timeout": 30
    },
    "imageGen": {
      "enabled": false,
      "defaultProvider": "openai"
    },
    "mcp": {
      "servers": {
        "example-server": {
          "command": "npx",
          "args": ["-y", "@example/mcp-server"],
          "env": {},
          "enabled": true
        }
      }
    }
  },
  "credentialHelper": {
    "command": "",
    "args": [],
    "format": "json"
  },
  "voice": {
    "transcription": {
      "enabled": false,
      "apiKey": "your-groq-api-key",
      "apiBase": "https://api.groq.com/openai/v1/audio/transcriptions",
      "model": "whisper-large-v3-turbo",
      "localModelPath": "",
      "preferLocal": true,
      "threads": 4
    }
  }
